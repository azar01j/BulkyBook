@model IEnumerable<Category>
@{

    ViewData["Title"] = "Index";

}

<h1>Category Available</h1>

<table class="table table-bordered table-stripped" style="width:100%">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
            <th>
                Created Date
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="60%">
                    @obj.Name
                </td>
                <td width="60%">
                    @obj.DisplayOrder
                </td>
                <td>
                    @obj.CreatedDate
                </td>
            </tr>
        }
    </tbody>

</table>